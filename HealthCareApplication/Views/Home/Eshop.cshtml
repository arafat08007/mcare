<div class="container">
    <div class="row">

                  <div class="col-md-12">
                     <div id="demo" class="box jplist" style="margin: 20px 0 0 0">
                        <!-- ios button: show/hide panel -->
                        <div class="jplist-ios-button">
                           <i class="fa fa-sort"></i>
                           Filter Search
                        </div>
                        <!-- panel -->
                        <div class="jplist-panel box panel-top">
                           <!-- reset button -->
                           <button type="button" class="jplist-reset-btn" data-control-type="reset" data-control-name="reset" data-control-action="reset">Reset &nbsp;<i class="fa fa-share"></i></button>
                           <!-- items per page dropdown -->
                           <div 
                              class="jplist-drop-down" 
                              data-control-type="items-per-page-drop-down" 
                              data-control-name="paging" 
                              data-control-action="paging">
                              <ul>
                                 <li><span data-number="3"> 3 per page </span></li>
                                 <li><span data-number="5"> 5 per page </span></li>
                                 <li><span data-number="10" data-default="true"> 10 per page </span></li>
                                 <li><span data-number="20"> 20 per page </span></li>
                                 <li><span data-number="all"> View All </span></li>
                              </ul>
                           </div>
                           <!-- sort dropdown -->
                           <div class="jplist-drop-down" data-control-type="sort-drop-down" data-control-name="sort" data-control-action="sort" data-datetime-format="{month}/{day}/{year}">
                              <!-- {year}, {month}, {day}, {hour}, {min}, {sec} -->
                              <ul>
                                 <li><span data-path="default">Sort by</span></li>
                                 <li><span data-path=".title" data-order="asc" data-type="text">Title A-Z</span></li>
                                 <li><span data-path=".title" data-order="cat" data-type="text">Title Z-A</span></li>
                                 <li><span data-path=".cat" data-order="asc" data-type="text">category A-Z</span></li>
                                 <li><span data-path=".cat" data-order="cat" data-type="text">category Z-A</span></li>
                                 <!-- <li><span data-path=".like" data-order="asc" data-type="number" data-default="true">Likes asc</span></li>
                                    <li><span data-path=".like" data-order="cat" data-type="number">Likes cat</span></li>
                                    <li><span data-path=".date" data-order="asc" data-type="datetime">Date asc</span></li>
                                    <li><span data-path=".date" data-order="cat" data-type="datetime">Date cat</span></li> -->
                              </ul>
                           </div>
                           <div 
                              class="jplist-drop-down cat-dropdown" 
                              data-control-type="filter-drop-down" 
                              data-control-name="category-filter" 
                              data-control-action="filter">
                              <ul>
                                 <li><span data-path=".ace">ACE inhibitors with thiazides</span></li>
                                 <li><span data-path=".mac"  data-default="true">Macrolide derivatives</span></li>
                                 <li><span data-path=".nar">Narcotic analgesic combination</span></li>
                              </ul>
                           </div>
                           <!-- filter by title -->
                           <div class="text-filter-box">
                              <i class="fa fa-search  jplist-icon"></i>
                              <input data-path=".title" type="text" value="" placeholder="Filter by Title" data-control-type="textbox" data-control-name="title-filter" data-control-action="filter" />
                           </div>
                           @*<!-- filter by category -->
