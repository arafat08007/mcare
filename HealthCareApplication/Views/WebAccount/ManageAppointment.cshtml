@{
    ViewBag.Title = "ManageAppointment";
    ViewBag.ActiveMy = "active";
    Layout = "~/Views/Shared/_LayoutSite.cshtml";
}
@model HealthCareApplication.Models.SiteInfo

<div class="container">
    <div class="row">
        <div class="col-md-4 mt-30 mb-20">
          @Html.Raw(ViewBag.SiteMyMenuList)
        </div>
        <div class="col-md-8 mt-30 mb-20">
            @*Content Area Start*@

                @Html.Raw(ViewBag.AppointmentData)
                  
            @*Content Area End*@
        </div>
    </div>
</div>

<script type="text/javascript">
    function DeleteAppointment(id) {
        if (confirm("Are you sure to Cancel this?"))
            $.post('/WebAccount/DeleteAppointment/' + id, function (data) { if (data.Success) location.reload(); else alert(data.Message); });
    }
</script>